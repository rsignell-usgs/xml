<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0 http://www.unidata.ucar.edu/schemas/thredds/InvCatalog.1.0.3.xsd"
  xmlns="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0"
  xmlns:xlink="http://www.w3.org/1999/xlink" name="BIO-WW3 Catalog">
  <service name="allServices" serviceType="Compound" base="">
    <service name="ncdods" serviceType="OPENDAP" base="/thredds/dodsC/"/>
    <service name="wcs" serviceType="WCS" base="/thredds/wcs/"/>
    <service name="wms" serviceType="WMS" base="/thredds/wms/"/>
    <service name="ncss" serviceType="NetcdfSubset" base="/thredds/ncss/grid/"/>
  </service>

  <datasetRoot path="WW3" location="/usgs/data0/rsignell/models/bio_ww3/efine_nc_last/"
    cache="false"/>

  <dataset name="BIO-WW3">

    <metadata inherited="true">
      <dataType>Grid</dataType>
      <dataFormat>NetCDF</dataFormat>
      <serviceName>allServices</serviceName>
      <creator>
        <name vocabulary="DIF">Will Perrie</name>
        <contact url="http://www.dfo-mpo.gc.ca" email="PerrieW@mar.dfo-mpo.gc.ca"/>
      </creator>
      <documentation type="summary"> WaveWatch III ocean wave model products from Will Perry and
        Bechara Toulany, Bedford Institute of Oceanography, Canada. </documentation>
    </metadata>

    <featureCollection featureType="FMRC" name="WaveWatch III Forecast Collection (Gulf of Maine)"
      harvest="true" path="bio/ww3/fmrc">
      <metadata inherited="true">
        <documentation xlink:href="http://scoop.sura.org/Catalog/details.pl?collectionid=sbioww3f"
          xlink:title="SCOOP Catalog Details"/>
      </metadata>
      <collection spec="/usgs/data0/rsignell/models/bio_ww3/efine_nc/#yyyyMMddHH#.nc"
        olderThan="1 min"/>
      <update startup="true" rescan="0 30 8/20 * * ? *" trigger="allow"/>

      <protoDataset choice="First">
        <netcdf xmlns="http://www.unidata.ucar.edu/namespaces/netcdf/ncml-2.2">
          <variable name="hs">
            <attribute name="_FillValue" type="float" value="-999."/>
          </variable>
          <variable name="swp">
            <attribute name="_FillValue" type="float" value="-999."/>
          </variable>
          <variable name="dir">
            <attribute name="_FillValue" type="float" value="-999."/>
          </variable>
        </netcdf>
      </protoDataset>

      <fmrcConfig regularize="false" datasetTypes="Best"/>
    </featureCollection>
    
    <featureCollection featureType="FMRC" name="WaveWatch III Forecast last 3 days (Gulf of Maine)"
      harvest="true" path="bio/ww3/fmrc3">
      <metadata inherited="true">
        <documentation xlink:href="http://scoop.sura.org/Catalog/details.pl?collectionid=sbioww3f"
          xlink:title="SCOOP Catalog Details"/>
      </metadata>
      <collection spec="/usgs/data0/rsignell/models/bio_ww3/efine_nc_latest/#yyyyMMddHH#.nc"
        olderThan="1 min"/>
      <update startup="true" rescan="0 30 8/20 * * ? *" trigger="allow"/>
      
      <protoDataset choice="First">
        <netcdf xmlns="http://www.unidata.ucar.edu/namespaces/netcdf/ncml-2.2">
          <variable name="hs">
            <attribute name="_FillValue" type="float" value="-999."/>
          </variable>
          <variable name="swp">
            <attribute name="_FillValue" type="float" value="-999."/>
          </variable>
          <variable name="dir">
            <attribute name="_FillValue" type="float" value="-999."/>
          </variable>
        </netcdf>
      </protoDataset>
      
      <fmrcConfig regularize="false" datasetTypes="Best"/>
    </featureCollection>


    <dataset name="WaveWatch III Forecast latest forecast (Gulf of Maine)" ID="ww3/fine"
      urlPath="WW3/fine.nc" dataType="Grid"/>

  </dataset>
</catalog>
